{
  "manifest_version": 2,

  "name": "gh-freshness",
  "description": "Alert that looking at the old Github repository.",
  "version": "0.1.0",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://github.com/*", "https://github.com/*/*/tree/*"],
      "js": ["jquery.min.js","moment.min.js","script.js"],
      "run_at": "document_start"
    }
  ]
}
